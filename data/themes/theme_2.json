        {
            "text_shadow_size": "1",
            "text_shadow_offset": "0,0"
        }
    },
    "#everything_window.screen_space_health_bar":
    {
        "colours":
        {
            "normal_text": "#FFFFFF",
            "filled_bar": "#0091fbAA,#2060A0AA,90",
