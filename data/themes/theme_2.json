{
    "defaults":
    {
        "colours":
        {
            "normal_bg": "#fb9e91",
            "hovered_bg": "#fbda91",
            "disabled_bg": "#cb6e61",
            "selected_bg": "#fb0091",
            "dark_bg": "#fb8691",
            "disabled_dark_bg": "#db6671",
            "normal_text": "#202020",
            "hovered_text": "#303030",
            "selected_text": "#404040",
            "disabled_text": "#000000",
            "link_text": "#0000EE",
            "link_hover": "#2020FF",
            "link_selected": "#551A8B",
            "text_shadow": "#777777",
            "normal_border": "#ffB2A5",
            "hovered_border": "#ffC2B5",
            "disabled_border": "#Df9285",
            "selected_border": "#ff00A1",
            "filled_bar": "#f4251b",
            "unfilled_bar": "#CCCCCC"
        }
    },

    "#test_prototype_colours":
    {
        "colours":
        {
            "dark_bg": "#fb86fb,#fd92A0,180",
            "normal_border": "#ffB2A5",
            "normal_bg": "#fb9e91"
        }
    },

    "#test_prototype_font":
    {
        "font":
        {
            "name": "montserrat",
            "size": "12",
            "bold": "1",
            "italic": "0",
            "regular_resource": { "package": "data.fonts", "resource": "Montserrat-Regular.ttf"},
            "bold_resource": { "package": "data.fonts", "resource": "Montserrat-Bold.ttf"},
            "italic_resource": { "package": "data.fonts", "resource": "Montserrat-Italic.ttf"},
            "bold_italic_resource": { "package": "data.fonts", "resource": "Montserrat-BoldItalic.ttf"}
        }
    },

    "#test_prototype_image":
    {
        "images":
        {
            "normal_image": {
                "package": "data.images",
                "resource": "splat.bmp",
                "sub_surface_rect": "0,0,32,32"
            },
            "hovered_image": {
                "package": "data.images",
                "resource": "splat.bmp",
                "sub_surface_rect": "32,0,32,32"
            }
        }
    },

    "horizontal_slider":
    {
        "prototype": "#test_prototype_colours",

        "colours":
        {
            "dark_bg": "#fb86fb,#fd92A0,90"
        },
        "misc":
        {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "10",
            "shadow_width": "2",
            "border_width": "1",
            "enable_arrow_buttons": "1"
        }
    },
    "horizontal_slider.@arrow_button":
    {
        "misc":
        {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "8",
            "text_horiz_alignment_padding": "2"
        }
    },
    "vertical_scroll_bar":
    {
        "colours":
        {
            "selected_bg": "#fb9e91",
            "dark_bg": "#fb8691"
        }
    },

    "tool_tip.text_box":
    {
        "colours":
        {
            "dark_bg": "#fb9e91B0"
        },
        "misc":
        {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "10",
            "padding": "8,8"
        }
    },

    "#hover_me_button":
    {
        "prototype": "#test_prototype_image",

        "misc":
        {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "18",
            "shadow_width": "2",
            "border_width": "1",
            "state_transitions":
            {
                "normal_hovered": "0.2",
                "hovered_normal": "0.5"
            }
        }
    },

    "#everything_button":
    {
        "prototype": "#test_prototype_image",

        "colours":
        {
            "normal_border": "#ffB2A5",
            "normal_text": "#FFFFFF,#FFA0A0,90"
        },
        "misc":
        {
            "shape": "ellipse",
            "shadow_width": "2",
            "border_width": "1"
        }
    },

    "text_entry_line":
    {
        "prototype": "#test_prototype_font",
        "colours":
        {
            "selected_bg": "#fb0091A0,#20fdA0A0,0",
            "normal_text": "#FFFFFF,#FFA0A0,90",
            "selected_text": "#FFFFFF,#A0A0FF,90"
        },
        "misc":
        {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "10",
            "border_width": "1",
            "padding": "2,0"
        }
    },

    "#drop_down_shape_prototype": {
        "misc": {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "4"
        }
    },
    "#drop_down_text_prototype":
    {
        "font":
        {
                "name": "montserrat",
                "size": "12",
                "bold": "0",
                "italic": "1",
                "regular_resource": { "package": "data.fonts", "resource": "Montserrat-Regular.ttf"},
                "bold_resource": { "package": "data.fonts", "resource": "Montserrat-Bold.ttf"},
                "italic_resource": { "package": "data.fonts", "resource": "Montserrat-Italic.ttf"},
                "bold_italic_resource": { "package": "data.fonts", "resource": "Montserrat-BoldItalic.ttf"}

        }
    },
    "drop_down_menu":
    {
        "prototype": "#drop_down_shape_prototype",

        "colours":
        {
            "dark_bg": "#fb9e91",
            "normal_border": "#FFB0B0,#FFB0FF,0"
        },
        "misc":
        {
            "shadow_width": "2",
            "border_width": "1",
            "expand_direction": "up",
            "open_button_width": "0"
        }
    },

    "drop_down_menu.#drop_down_options_list":
    {
        "prototype": "#drop_down_shape_prototype",

        "misc":
        {
            "shadow_width": "2",
            "border_width": "1"
        }
    },

    "drop_down_menu.#drop_down_options_list.vertical_scroll_bar":
    {
        "misc":
        {
            "shadow_width": "0",
            "border_width": "1",
            "enable_arrow_buttons": "0"
        }
    },

    "drop_down_menu.#drop_down_options_list.button":
    {
        "prototype": "#drop_down_shape_prototype",
        "prototype": "#drop_down_text_prototype",

        "misc":
        {
            "shadow_width": "0",
            "border_width": "0",

            "state_transitions":
            {
                "normal_hovered": "0.2",
                "hovered_normal": "0.5"
            }
        }
    },

    "drop_down_menu.button":
    {
        "prototype": "#drop_down_shape_prototype",
        "prototype": "#drop_down_text_prototype",

        "misc":
        {
            "shadow_width": "0",
            "border_width": "0",
            "state_transitions":
            {
                "normal_hovered": "0.2",
                "hovered_normal": "0.5"
            }
        }
    },

    "screen_space_health_bar":
    {
        "colours":
        {
            "normal_text": "#FFFFFF"
        }
    },

    "#base_window":
    {
        "misc":
        {
            "border_width": "8"
        }
    },

    "#scaling_window":
    {
        "prototype": "#base_window",
        "misc":
        {
            "shadow_width": "1"
        }
    },

    "#everything_window.horizontal_slider":
    {
        "colours":
        {
            "dark_bg": "#fb8691,#fd92A0,180",
            "normal_border": "#ffB2A5",
            "normal_bg": "#fb9e91"
        },
        "misc":
        {
            "shape": "rounded_rectangle",
            "border_width": "2",
            "shadow_width": "1",
            "enable_arrow_buttons": "1"
        }
    },
    "#everything_window.horizontal_slider.#sliding_button":
    {
        "misc":
        {
            "shape": "ellipse",
            "border_width": "1",
            "shadow_width": "1"
        }
    },
    "#everything_window.#title_bar":
    {
        "font":
        {
            "name": "montserrat",
            "size": "12",
            "bold": "0",
            "italic": "1",
            "regular_resource": { "package": "data.fonts", "resource": "Montserrat-Regular.ttf"},
            "bold_resource": { "package": "data.fonts", "resource": "Montserrat-Bold.ttf"},
            "italic_resource": { "package": "data.fonts", "resource": "Montserrat-Italic.ttf"},
            "bold_italic_resource": { "package": "data.fonts", "resource": "Montserrat-BoldItalic.ttf"}
        },
        "misc":
