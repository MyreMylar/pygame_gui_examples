                "resource": "splat.bmp",
                "sub_surface_rect": "0,0,32,32"
            }
        }
    },
    "#fps_counter":
    {
        "colours":
        {
            "normal_text": "#FFFFFF",
            "dark_bg": "#fb8691"
        },
        "font":
        {
            "name": "fira_code",
            "size": "32"
        }
    },
    "#frame_timer":
    {
        "colours":
        {
            "normal_text": "#FFFFFF",
            "dark_bg": "#00000000"
